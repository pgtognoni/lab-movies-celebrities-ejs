<h2 class='new-celebrity-title'>New Celebrity</h2>
<form class="new-cel-form" method="POST" action="/celebrities/create">
    <label for="name">Name 
        <input type="text" name="name" id="name"/>
    </label>
    <label for="occupation"> 
        Actor <input type="radio" name="occupation" value="actor"/>
        Singer <input type="radio" name="occupation" value="singer"/>
        Comedian <input type="radio" name="occupation" value="comedian"/>
        unknown <input type="radio" name="occupation" value="unknown"/>
    </label>
    <label for="catchphrase">CatchPhrase 
        <input type="text" name="catchphrase" />
    </label>
    <button type="submit">Submit</button>
</form>